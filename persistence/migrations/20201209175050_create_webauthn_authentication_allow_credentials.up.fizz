create_table("webauthn_authentication_allow_credentials") {
	t.Column("id", "uuid", {primary: true})
	t.Column("credential_id", "string", {"size": 255})
	t.Column("authentication_request_id", "uuid")
	t.ForeignKey("authentication_request_id", {"webauthn_authentication_requests": ["id"]}, {"on_delete": "cascade", "on_update": "cascade"})
	t.Timestamps()
}
